             	</owl:onProperty>			  	<owl:maxCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:maxCardinality>			</owl:Restriction>
		</rdfs:subClassOf>
		
		<!-- Human Provenance -->
