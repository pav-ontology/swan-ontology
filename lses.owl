           </owl:Restriction>
    	</rdfs:subClassOf>
    	<rdfs:subClassOf>
			<owl:Restriction>
	           	<owl:onProperty>
               		<owl:ObjectProperty rdf:about="&pav;createdBy"/>
             	</owl:onProperty>
             	<owl:maxCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:maxCardinality>
           </owl:Restriction>
    	</rdfs:subClassOf>
    	
    	<rdfs:subClassOf>
			<owl:Restriction>			  	<owl:onProperty>
