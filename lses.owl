        </rdfs:domain>
	    <rdf:type rdf:resource="&owl;DatatypeProperty"/>
  	</owl:FunctionalProperty>    
    
	<owl:DatatypeProperty rdf:about="name">
	    <rdfs:range rdf:resource="&xsd;string"/>
	    <rdfs:domain>
	      <owl:Class>
